{
  "name": "playwright-poc",
  "version": "1.0.0",
  "description": "Test core repo with Playwright and TypeScript",
  "main": "index.js",
  "scripts": {
    "test:chrome": "playwright test --config=playwright.config.ts --project=chromium",
    "test:firefox": "playwright test --config=playwright.config.ts --project=firefox",
    "test:webkit": "playwright test --config=playwright.config.ts --project=webkit",
    "test:e2e": "playwright test --config=playwright.config.ts --reporter=html",
    "test:e2e:exchange": "playwright test --config=playwright.config.ts --grep @exchange",
    "test:e2e:feedback": "playwright test --config=playwright.config.ts --grep @feedback",
    "test:e2e:funds": "playwright test --config=playwright.config.ts --grep @funds",
    "test:e2e:login": "playwright test --config=playwright.config.ts --grep @login",
    "test:e2e:payments": "playwright test --config=playwright.config.ts --grep @payments",
    "test:e2e:searchbox": "playwright test --config=playwright.config.ts --grep @searchbox",
    "test:e2e:transactions": "playwright test --config=playwright.config.ts --grep @transactions"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michalkasiarz/playwright-poc.git"
  },
  "keywords": [
    "playwright",
    "test",
    "automation"
  ],
  "author": "Michal Kasiarz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michalkasiarz/playwright-poc/issues"
  },
  "homepage": "https://github.com/michalkasiarz/playwright-poc#readme",
  "dependencies": {
    "@playwright/test": "^1.42.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "prettier": "^2.7.1"
  }
}
